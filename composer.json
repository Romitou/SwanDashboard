{
    "name": "romitou/swandashboard",
    "type": "project",
    "description": "Dashboard using MongoDB and Laravel for Swan.",
    "keywords": [
        "laravel",
        "mongodb",
        "swan"
    ],
    "version": "1.0.0",
    "license": "GPL-3.0-only",
    "homepage": "https://github.com/Romitou/SwanDashboard",
    "readme": "https://github.com/Romitou/SwanDashboard#readme",
    "authors": [
        {
            "name": "Romitou",
            "email": "romitou@protonmail.com",
            "homepage": "https://github.com/Romitou",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Romitou/SwanDashboard/issues",
        "wiki": "https://github.com/Romitou/SwanDashboard/wiki",
        "source": "https://github.com/Romitou/SwanDashboard"
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-pdo": "*",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/mongodb": "^3.8",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "sentry/sentry-laravel": "^1.8",
        "socialiteproviders/discord": "^2.0",
        "symfony/http-kernel": "5.1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.7",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
